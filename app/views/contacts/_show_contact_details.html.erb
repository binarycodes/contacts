<div class="contact_data_details">
  <div class="contact_name_details">
    <%= link_to "#{target.first_name} #{target.last_name}", target %>
  </div>
  <div class="contact_phones_details">
    <% for phone in target.phones %>
      <p>
        <img src="<%= asset_path(phone.phone_type.downcase+".png") %>" /> <%= phone.phone_no %>
      </p>
    <% end %>
  </div>
  <div class="contact_remarks_details">
    <%= target.remarks %>
  </div>
  <div class="contact_admin_details">
    <%= link_to 'Edit', edit_contact_path(target)  %> |
    <%= link_to 'Delete', target, :method => 'delete', :confirm => "Are you sure?", :class => "delete_contact" %>
  </div>
</div>
